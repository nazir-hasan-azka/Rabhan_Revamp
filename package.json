    "next": "^15.5.4",
    "next-auth": "^5.0.0-beta.29",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
